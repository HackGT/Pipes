{
    "version": "2.0.0",
    "plugins": {
        "Static": "../plugins/static",
        "Console": "../plugins/console",
        "Capitalization": "../plugins/capitalization",
        "Encrypt": "../plugins/encrypt",
        "Decrypt": "../plugins/decrypt"
    },
    "nodes": {
        "static": {
            "type": "Static",
            "arguments": ["Hello world from a graph file!"]
        },
        "password": {
            "type": "Static",
            "name": "Password Setter",
            "arguments": ["passw0rd"]
        },
        "capitalization": {
            "type": "Capitalization"
        },
        "encrypt": {
            "type": "Encrypt"
        },
        "decrypt": {
            "type": "Decrypt"
        },
        "log_1": {
            "type": "Console"
        },
        "log_2": {
            "type": "Console"
        },
        "log_3": {
            "type": "Console"
        }
    },
    "connections": [
        {
            "from": "password", "to": "encrypt",
            "mapping": {
                "static_data": "password"
            }
        },
        {
            "from": "password", "to": "decrypt",
            "mapping": {
                "static_data": "password"
            }
        },
        {
            "from": "static", "to": "capitalization",
            "mapping": {
                "static_data": "text"
            }
        },
        {
            "from": "capitalization", "to": "log_1",
            "mapping": {
                "capitalized": "console_data"
            }
        },
        {
            "from": "log_1", "to": "encrypt",
            "mapping": {
                "console_data": "data"
            }
        },
        {
            "from": "encrypt", "to": "log_2",
            "mapping": {
                "encryptedSimple": "console_data"
            }
        },
        {
            "from": "encrypt", "to": "decrypt",
            "mapping": {
                "encryptionResult": "encryptionResult"
            }
        },
        {
            "from": "decrypt", "to": "log_3",
            "mapping": {
                "data": "console_data"
            }
        }
    ]
}
